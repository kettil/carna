{
  "deps": {
    "ignore": {
      "packages": [
        "@kettil/commitlint-config",
        "@kettil/semantic-release-config",
        "@commitlint/cli",
        "eslint",
        "husky",
        "prettier",
        "semantic-release",

        "@babel/cli",
        "@babel/node",
        "babel-jest",
        "babel-loader",
        "babel-watch",
        "pino-pretty",

        "@types/jest",
        "jest",

        "eslint-plugin-jest",

        "carna"
      ]
    }
  },
  "license": {
    "replaceAliaseList": false,
    "replaceCompatibleList": false,
    "replaceHeuristicList": false,

    "aliases": {},
    "compatibilities": {},
    "heuristics": {},

    "ignore": {
      "packages": []
    }
  },
  "log": {
    "ignore": {
      "keys": []
    }
  },
  "test": {
    "coverage": {
      "threshold": {
        "statements": [50, 95],
        "branches": [50, 95],
        "functions": [50, 95],
        "lines": [50, 95]
      }
    }
  }
}
